campaignTrail_temp.multiple_endings = true;

function setImage(url) {
	if(url == '' || url == null) return;
	let interval = setInterval(function () {
		img = document.getElementsByClassName("person_image")[0];
		if (img != null) {
			img.src = url;
			clearInterval(interval);
		}
	}, 50);
}

current_slide = 0;
slides = [];

prev_slide = function() {
	if (current_slide > 0) {
		current_slide -= 1;
		document.getElementById("final_results_description").innerHTML = slides[current_slide][1];
		setImage(slides[current_slide][0]);
	}
}

next_slide = function() {
	if (current_slide < slides.length - 1) {
		current_slide += 1;
		document.getElementById("final_results_description").innerHTML = slides[current_slide][1];
		setImage(slides[current_slide][0]);
	}
}

function add_slide(image, slide_content, first, last) {
	if (!first) {
		slide_content += `<button style='float:left;' onclick='prev_slide()'>Previous Page</button></div>`;
	}
	if (!last) {
		slide_content += `<button style='float:left;'onclick='next_slide()'>Next Page</button></div>`;
	}
	if (!first && !last) {
		slide_content += `</div>`;
	}
	slides.push([image, slide_content]);
}

