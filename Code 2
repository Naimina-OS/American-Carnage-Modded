	outer_game_window_color = "rgb(138, 163, 178)";
	outer_descriptive_window_color = "#aec8d5";
	outer_container_color = "rgb(55, 77, 72)";
	header_img_url = "https://i.imgur.com/CZPCxxR.png";
	background_img_url = "https://i.imgur.com/pIIxcZe.jpeg";
	results_container_color = "#aec8d5"; //Running Mate Description Window color
	question_window_color = "#F8F8F8";
	question_window_background = "";
	question_window_text_color = "black";
	question_background_color = "#E8FBFF";
	progress_window_color = "white";
	progress_bar_text_color = "black";
	
	applyNewTheme();
}

